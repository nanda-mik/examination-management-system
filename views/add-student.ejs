<%- include('includes/head.ejs') %>
     <main>
        <div class="container py-md-5">
            <div class="row align-items-center">
              <div class="col-lg-5 pl-lg-5 pb-3 py-lg-5">
                <form class="addstudent-form" action="/admin/add-student" method="POST">
                    <div class="form-group">
                        <label for="name">Name of Student</label>
                        <input type="text" name="name"  class="form-control form-control-sm input-dark" autocomplete="off" placeholder="name" id="name">
                    </div>
                    <div class="form-group">
                        <label for="id">Enter college id</label>
                        <input type="text" name="id"  class="form-control form-control-sm input-dark" autocomplete="off" placeholder="college id" id="id">
                    </div>
                    <div class="form-group">
                        <label for="branch">Enter branch</label>
                        <input type="text" name="branch"  class="form-control form-control-sm input-dark" autocomplete="off" placeholder="branch" id="branch">
                    </div>
                    <div class="form-group">
                        <label for="year">Enter year</label>
                        <input type="text" name="year"  class="form-control form-control-sm input-dark" autocomplete="off" placeholder="year" id="year"> 
                    </div>
                    <button class="py-3 mt-4 btn btn-lg btn-success btn-block" type="submit">Add student Details</button>
                </form>
              </div>
            </div>
        </div>
     </main>

 <%- include('includes/end.ejs') %>